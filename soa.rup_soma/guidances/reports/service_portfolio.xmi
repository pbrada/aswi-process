            &lt;/td>&#xD;
            &lt;td>&#xD;
                Aligned, Composable, Described, Reusable&#xD;
            &lt;/td>&#xD;
        &lt;/tr>&#xD;
        &lt;tr>&#xD;
            &lt;td>&#xD;
                CustomerCreditAuthorization&#xD;
            &lt;/td>&#xD;
            &lt;td>&#xD;
                accepted&#xD;
            &lt;/td>&#xD;
