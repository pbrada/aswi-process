<org.eclipse.epf.uma:TaskDescription xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:org.eclipse.epf.uma="http://www.eclipse.org/epf/uma/1.0.6/uma.ecore" xmlns:epf="http://www.eclipse.org/epf" epf:version="1.5.1" xmlns:rmc="http://www.ibm.com/rmc" rmc:version="7.5.1" xmi:id="_0h4yAtnmEdmO6L4XMImrsA" name="find_business_actors_and_use_cases,{1CCEB101-0027-451C-81CA-521D8B127F98}" guid="_0h4yAtnmEdmO6L4XMImrsA" changeDate="2005-10-12T11:23:38.935-0700" version="7.1.0">
