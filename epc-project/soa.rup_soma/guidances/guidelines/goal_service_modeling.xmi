            &lt;td>&#xD;
                &amp;lt;KPI&amp;gt;&#xD;
            &lt;/td>&#xD;
            &lt;td>&#xD;
                &amp;lt;Metric&amp;gt;&#xD;
            &lt;/td>&#xD;
            &lt;td>&#xD;
                &amp;lt;Service&amp;gt;&#xD;
            &lt;/td>&#xD;
        &lt;/tr>&#xD;
        &lt;tr>&#xD;
            &lt;td>&#xD;
                2.1 &amp;lt;Sub-Goal&amp;gt;&#xD;
            &lt;/td>&#xD;
            &lt;td>&#xD;
                &amp;lt;KPI&amp;gt;&#xD;
            &lt;/td>&#xD;
