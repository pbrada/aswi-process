          <contentElements xsi:type="org.eclipse.epf.uma:Artifact" xmi:id="{E24679B7-19F1-483B-A1F1-578839C43888}" name="soa_service_model" guid="{E24679B7-19F1-483B-A1F1-578839C43888}" presentationName="Service Model" briefDescription="The service model is a model of the core elements of a Service Oriented Architecture (SOA). The service model is used as an essential input to tasks in implementation and testing." shapeicon="workproducts/resources/soa_svc_model_48.gif" nodeicon="workproducts/resources/soa_svc_model_16.gif" checklists="_Lw5NMDL-EduWI_fHNNtVQA" examples="1.6487122362967558E-307" reports="1.0870125010094147E-305 1.2262765587812462E-305" templates="_ZYHekE3eEduBbftZUgDxnQ _-bpYcE3aEdupj44ytPg0iA">
            <presentation xmi:id="_qt6iEPS3EdmIX5Pw7v8pRw" href="uma://_qt6iEPS3EdmIX5Pw7v8pRw#_qt6iEPS3EdmIX5Pw7v8pRw"/>
