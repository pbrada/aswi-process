            <conceptsAndPapers href="uma://_875O8A6hEdqg5YAaNtnnCw#_vHtFUC7CEdu64PK-GeN94Q"/>
            <containedArtifacts xmi:id="{DD9F96C3-4D00-4441-951E-2674B9AEE1C6}" name="soa_svce_model_message" guid="{DD9F96C3-4D00-4441-951E-2674B9AEE1C6}" presentationName="Message" briefDescription="This artifact is a container which identifies a subset of an information model or domain model which is passed into or out of a service invocation. A message is always passed by value and should have no defined behavior." shapeicon="workproducts/resources/soa_message_48.gif" nodeicon="workproducts/resources/soa_message_16.gif" guidelines="7.676056734461877E-306 1.6727924656888407E-305">
              <presentation xmi:id="_qzEH0PS3EdmIX5Pw7v8pRw" href="uma://_qzEH0PS3EdmIX5Pw7v8pRw#_qzEH0PS3EdmIX5Pw7v8pRw"/>
            </containedArtifacts>
            <containedArtifacts xmi:id="{FF65B0A2-6C53-4F01-9727-AACDB0D542C8}" name="soa_svce_model_service" guid="{FF65B0A2-6C53-4F01-9727-AACDB0D542C8}" presentationName="Service" briefDescription="This artifact is a model of the core elements of a Service Oriented Architecture (SOA). A service is provided by a service provider and is an instance of a service specification." shapeicon="workproducts/resources/soa_service_48.gif" nodeicon="workproducts/resources/soa_service_16.gif" conceptsAndPapers="7.723783423994501E-306" guidelines="2.1757522381974088E-306 1.6727924656888407E-305">
              <presentation xmi:id="_q1dUgPS3EdmIX5Pw7v8pRw" href="uma://_q1dUgPS3EdmIX5Pw7v8pRw#_q1dUgPS3EdmIX5Pw7v8pRw"/>
            </containedArtifacts>
            <containedArtifacts xmi:id="{95AA7C70-6259-4627-B705-6A67E33A47BC}" name="soa_svce_model_svce_chan" guid="{95AA7C70-6259-4627-B705-6A67E33A47BC}" presentationName="Service Channel" briefDescription="This artifact is a model element that represents a connection between two services or between a client and a server over which interaction with the service takes place. Note that the channel does not represent any interaction in particular." shapeicon="workproducts/resources/soa_svc_channel_48.gif" nodeicon="workproducts/resources/soa_svc_channel_16.gif" guidelines="2.5614739075754752E-306">
              <presentation xmi:id="_q38AwPS3EdmIX5Pw7v8pRw" href="uma://_q38AwPS3EdmIX5Pw7v8pRw#_q38AwPS3EdmIX5Pw7v8pRw"/>
            </containedArtifacts>
            <containedArtifacts xmi:id="{9BBBE5B0-4D39-4555-9E20-0ADBD8327D29}" name="soa_svce_model_svce_collab" guid="{9BBBE5B0-4D39-4555-9E20-0ADBD8327D29}" presentationName="Service Contract" briefDescription="This artifact is a representation of some set of communication between two or more services usually encapsulated as a new service. In this way, the model can represent services whose implementation is simply the collaboration of a set of existing services." shapeicon="workproducts/resources/soa_svc_collaboration_48.gif" nodeicon="workproducts/resources/soa_svc_collaboration_16.gif" guidelines="_QNuHcEV1Edqpo8Lgp7-Wmg">
              <presentation xmi:id="_q4_JoPS3EdmIX5Pw7v8pRw" href="uma://_q4_JoPS3EdmIX5Pw7v8pRw#_q4_JoPS3EdmIX5Pw7v8pRw"/>
            </containedArtifacts>
            <containedArtifacts xmi:id="{B0BF4414-0382-4605-9EE9-82F0DEC7292C}" name="soa_svce_model_svce_gtway" guid="{B0BF4414-0382-4605-9EE9-82F0DEC7292C}" presentationName="Service Gateway" briefDescription="This artifact looks very much like a service-model element except that it does not represent an end-point in terms of implementation of a service specification because it only forwards messages from the boundary of a service partition to a service enclosed within the partition. In this way, it allows for partitions to strictly define their interfaces in terms of service gateways." shapeicon="workproducts/resources/soa_svc_gateway_48.gif" nodeicon="workproducts/resources/soa_svc_gateway_16.gif" guidelines="2.5614739075754752E-306">
              <presentation xmi:id="_q5RdgPS3EdmIX5Pw7v8pRw" href="uma://_q5RdgPS3EdmIX5Pw7v8pRw#_q5RdgPS3EdmIX5Pw7v8pRw"/>
            </containedArtifacts>
            <containedArtifacts xmi:id="{C302AF5A-1591-4F26-94E5-C412866553BF}" name="soa_svce_model_svce_partn" guid="{C302AF5A-1591-4F26-94E5-C412866553BF}" presentationName="Service Partition" briefDescription="This artifact is a model element that provides a logical grouping for service providers. The grouping is logical in the sense that the partition structure may reflect a system structure that will impact the way the physical system is deployed or it may represent a structure that has no impact on deployment, such as the ownership of services by organizations." shapeicon="workproducts/resources/soa_svc_partition_48.gif" nodeicon="workproducts/resources/soa_svc_partition_16.gif" conceptsAndPapers="1.6501323286225543E-305">
              <presentation xmi:id="_q5dqwPS3EdmIX5Pw7v8pRw" href="uma://_q5dqwPS3EdmIX5Pw7v8pRw#_q5dqwPS3EdmIX5Pw7v8pRw"/>
            </containedArtifacts>
            <containedArtifacts xmi:id="{8427751F-3C14-4CEB-9816-5112BDB91EED}" name="soa_svce_model_svce_provider" guid="{8427751F-3C14-4CEB-9816-5112BDB91EED}" presentationName="Service Provider" briefDescription="This artifact is a model element that groups a related set of services." shapeicon="workproducts/resources/soa_svc_provider_48.gif" nodeicon="workproducts/resources/soa_svc_provider_16.gif">
              <presentation xmi:id="_q5jxYPS3EdmIX5Pw7v8pRw" href="uma://_q5jxYPS3EdmIX5Pw7v8pRw#_q5jxYPS3EdmIX5Pw7v8pRw"/>
            </containedArtifacts>
            <containedArtifacts xmi:id="{20F06B5E-95D5-422C-AB68-7C213D28533A}" name="soa_svce_model_svce_spec" guid="{20F06B5E-95D5-422C-AB68-7C213D28533A}" presentationName="Service Specification" briefDescription="This artifact is a model element that provides both the structural and behavioral specification for a service instance. A service specification also may identify a set of policies governing the access to a service or use of the service. This specification acts as a contract between the service client and service implementer; the client understands how to interact with a service and the implementer understands the behavior expected of the implementation." shapeicon="workproducts/resources/soa_svc_specification_48.gif" nodeicon="workproducts/resources/soa_svc_specification_16.gif" guidelines="4.2228409903513537E-306">
              <presentation xmi:id="_q5v-oPS3EdmIX5Pw7v8pRw" href="uma://_q5v-oPS3EdmIX5Pw7v8pRw#_q5v-oPS3EdmIX5Pw7v8pRw"/>
            </containedArtifacts>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Concept" xmi:id="1.851952881072508E-305" name="service_portfolio" guid="1.851952881072508E-305" presentationName="Service Portfolio" briefDescription="This concept describes the advantages of services in enabling organizations to promote reuse by composing applications from a portfolio of services. The categorization of services for effective storage, discovery and retrieval of services from repositories is also discussed.">
            <presentation xmi:id="_qv6tMPS3EdmIX5Pw7v8pRw" href="uma://_qv6tMPS3EdmIX5Pw7v8pRw#_qv6tMPS3EdmIX5Pw7v8pRw"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Report" xmi:id="1.0870125010094147E-305" name="service_portfolio" guid="1.0870125010094147E-305" presentationName="Service Portfolio" briefDescription="This report describes the service model comprehensively, in terms of the specifications detailed within it. The report is best used when the service model represents a comprehensive survey of the services provided by the enterprise and can therefore be seen as representing the portfolio of services in the enterprise. The report can be used to describe the service model at different stages: during elaboration; when you identify the first services and specifications and how they relate to the existing portfolio; and during construction, when the design is complete, to identify services added to the portfolio. This report is used by various people interested in the service model, such as software architects, use-case designers, designers, testers, reviewers, and managers.">
            <presentation xmi:id="_qxKDUPS3EdmIX5Pw7v8pRw" href="uma://_qxKDUPS3EdmIX5Pw7v8pRw#_qxKDUPS3EdmIX5Pw7v8pRw"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Report" xmi:id="1.2262765587812462E-305" name="service_dependencies" guid="1.2262765587812462E-305" presentationName="Service Dependencies" briefDescription="This report describes the service model comprehensively, in terms of the dependencies between services. If you are using packages, the document shows the model structure hierarchically. The report can be used to describe the entire service model at different stages: during elaboration; while looking at the existing service portfolio to identify dependencies on services you may use; and during construction, when the design is complete, to report on the dependencies on services you have designed. This report is used by various people interested in the service model such as software architects, use-case designers, designers, testers, reviewers, and managers.">
            <presentation xmi:id="_qxQJ8PS3EdmIX5Pw7v8pRw" href="uma://_qxQJ8PS3EdmIX5Pw7v8pRw#_qxQJ8PS3EdmIX5Pw7v8pRw"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Example" xmi:id="1.6487122362967558E-307" name="soa_model_example" guid="1.6487122362967558E-307" presentationName="Service-Oriented Architecture (SOA) Model Example" briefDescription="This is an example of the application of service identification, service design, and service realization techniques to construct a service model for a retail application.">
            <presentation xmi:id="_qylmsPS3EdmIX5Pw7v8pRw" href="uma://_qylmsPS3EdmIX5Pw7v8pRw#_qylmsPS3EdmIX5Pw7v8pRw"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Guideline" xmi:id="7.676056734461877E-306" name="message_attachments" guid="7.676056734461877E-306" presentationName="Message Attachments" briefDescription="This guideline presents ways of dealing with the transmission of large or otherwise complex elements as attachments to messages by reference or containment, giving the designer the ability to satisfy performance or bandwidth constraints.">
            <presentation xmi:id="_q0BKEPS3EdmIX5Pw7v8pRw" href="uma://_q0BKEPS3EdmIX5Pw7v8pRw#_q0BKEPS3EdmIX5Pw7v8pRw"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Guideline" xmi:id="1.6727924656888407E-305" name="service_data_encapsulation" guid="1.6727924656888407E-305" presentationName="Service Data Encapsulation" briefDescription="This guideline takes the idea of encapsulation as it is applied in object-oriented development and extends it to services. It further describes how communication with a service is done by message exchange, introducing the metaphor of services as 'fiefdoms,' which may interact directly only with known 'emissaries,' which, in turn, interact with the consumer.">
            <presentation xmi:id="_q0mY4PS3EdmIX5Pw7v8pRw" href="uma://_q0mY4PS3EdmIX5Pw7v8pRw#_q0mY4PS3EdmIX5Pw7v8pRw"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Guideline" xmi:id="2.1757522381974088E-306" name="service" guid="2.1757522381974088E-306" presentationName="Service" briefDescription="This guideline elaborates on the definition of a service as a discoverable software resource with an externalized service specification.">
            <presentation xmi:id="_q1jbIPS3EdmIX5Pw7v8pRw" href="uma://_q1jbIPS3EdmIX5Pw7v8pRw#_q1jbIPS3EdmIX5Pw7v8pRw"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Guideline" xmi:id="2.5614739075754752E-306" name="service_mediation" guid="2.5614739075754752E-306" presentationName="Service Mediation" briefDescription="This guideline describes ways in which communication between incompatible consumers and services is enabled through mediation, transforming consumer requests or protocols into forms that service providers can understand.">
            <presentation xmi:id="_q4IOAPS3EdmIX5Pw7v8pRw" href="uma://_q4IOAPS3EdmIX5Pw7v8pRw#_q4IOAPS3EdmIX5Pw7v8pRw"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Guideline" xmi:id="4.2228409903513537E-306" name="state_management_for_services" guid="4.2228409903513537E-306" presentationName="State Management for Services" briefDescription="In this guideline, issues surrounding the use of stateful and stateless services are explored in terms of their effect on performance and robustness, and also when the interaction requires a transactional approach. The notion of state management in an SOA environment can be considered to fall into three main categories: Transaction State, Security State and Functional State.">
            <presentation xmi:id="_q6CSgPS3EdmIX5Pw7v8pRw" href="uma://_q6CSgPS3EdmIX5Pw7v8pRw#_q6CSgPS3EdmIX5Pw7v8pRw"/>
          </contentElements>
