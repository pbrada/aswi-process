  <org.eclipse.epf.uma:ActivityDescription xmi:id="-jVef9p3HCkCaxjPHTRq44g" name="Capture Critical Solution Behavior,_zeJMoEGaEdq7A8v8S2_Mqw" guid="-jVef9p3HCkCaxjPHTRq44g"/>
