              <containedArtifacts xmi:id="{47F095EB-E651-4A34-B75B-BACE38ED90C7}" name="rup_business_use_case_realization" guid="{47F095EB-E651-4A34-B75B-BACE38ED90C7}" presentationName="Business Use-Case Realization" briefDescription="A Business Use-Case Realization describes how business systems, business workers, business entities, and business events collaborate to perform a particular business use case. " shapeicon="workproducts/resources/bucr.jpg" nodeicon="workproducts/resources/bucr.gif" checklists="1.3303501071086346E-305" guidelines="1.0660567082472948E-304" reports="4.1216383810562285E-306" templates="2.6576205146287937E-306">
